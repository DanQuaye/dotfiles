; .editorconfig

root = true

[**.js]
indent_style = space
indent_size = 4
jslint_happy = 1
keep_array_indentation = 1
space_before_conditional = 1
break_chained_methods = 1

[**.css]
indent_style = space
indent_size = 4

[**.html]
indent_style = space
indent_size = 4
max_char = 78
brace_style = expand
